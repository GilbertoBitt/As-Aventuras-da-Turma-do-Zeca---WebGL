  - 'CREATE VIEW DBORANKFILTER AS

    SELECT DBORANKING.idMinigame,DBORANKING.idUsuario,DBORANKING.highscore,DBOESCOLA.idEscola,DBOESCOLA.nomeEscola,DBOUSUARIOS.idTurma,DBOTURMA.descTurma,DBOUSUARIOS.idCliente,DBOUSUARIOS.login
