    INNER JOIN DBOTURMA ON DBOUSUARIOS.idTurma=DBOTURMA.idTurma INNER JOIN DBOESCOLA
    ON DBOTURMA.idEscola=DBOESCOLA.idEscola ORDER BY DBORANKING.highscore DESC'
